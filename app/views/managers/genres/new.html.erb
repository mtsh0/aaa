<%# render 'managers/partials/sidebar' %>

<div class="col-md-4 col-md-offset-4">
  <h3 class="text-center">ジャンル名登録</h3>

  <%= form_for @genre, url: managers_genres_path, method: :post do |f| %>

    <%= render partial: 'genre_form', locals: {f: f} %>

    <%= f.hidden_field :approval_id, value: 0 %>

    <div class="actions text-center">
      <%= f.submit '登録する', class: 'btn btn-default btn-md'%>
    </div>
  <% end %>
</div>



