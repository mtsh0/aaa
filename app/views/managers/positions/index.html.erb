<p id="notice"><%= notice %></p>

<h1>Listing Managers Positions</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @managers_positions.each do |managers_position| %>
      <tr>
        <td><%= managers_position.title %></td>
        <td><%= managers_position.description %></td>
        <td><%= managers_position.address %></td>
        <td><%= managers_position.latitude %></td>
        <td><%= managers_position.longitude %></td>
        <td><%= link_to 'Show', managers_position %></td>
        <td><%= link_to 'Edit', edit_managers_position_path(managers_position) %></td>
        <td><%= link_to 'Destroy', managers_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Position', new_managers_position_path %>
