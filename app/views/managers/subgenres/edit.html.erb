<%# render 'managers/partials/sidebar' %>

<div class="col-md-4 col-md-offset-4">
  <h3 class="text-center">サブジャンル変更</h3>

  <%= form_for @subgenre, url: managers_subgenre_path, method: :put do |f| %>

    <%= render partial: 'subgenre_form', locals: {f: f} %>

    <%= f.hidden_field :approval_id, value: 0 %>

    <div class="actions text-center">
      <%= f.submit '変更する', class: 'btn btn-default btn-md'%>
    </div>
  <% end %>
</div>
